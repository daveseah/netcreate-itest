{
  "name": "netcreate-itest",
  "version": "2.0.0-alpha",
  "description": "research project for network graph analysis",
  "workspaces": [
    "_ur/",
    "_ur_addons/"
  ],
  "scripts": {
    "dev": ". ./@build-ursys.sh && brunch w -s",
    "lint": "npm exec --workspaces -- npm run lint && eslint app/",
    "dev:inspect": "node --inspect brunch-server.js",
    "classroom": "brunch build -e classroom",
    "package": ". ./@build-ursys.sh && brunch build -e package",
    "package:debug": "brunch watch -e package_debug -s",
    "clean": "rm -rf ./public ./node_modules && npm exec --workspaces -- npm run clean",
    "debug": "LOGGY_STACKS=true BRUNCH_DEVTOOLS=true ./brunch-debug watch --server",
    "log": "tail -f -n100 \"$(ls -at ./runtime/logs/* | head -n 1)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daveseah/netcreate-itest.git"
  },
  "author": "Inquirium LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daveseah/netcreate-itest/issues"
  },
  "homepage": "https://github.com/daveseah/netcreate-itest#readme",
